{
  "name": "@ihf-rivendell/qa",
  "version": "0.3.82",
  "description": "IHF Rivendell QA",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run co:login && npx husky install || exit 0 ",
    "prebuild": "npx eslint . --fix --ext .ts",
    "co:login": "aws codeartifact login --tool npm --repository qa --domain rivendell",
    "lint": "eslint . --fix --ext .ts",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "tsc -d",
    "test": "jest --runInBand --coverage",
    "test:ci": "jest --runInBand --coverage --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://git-codecommit.us-east-2.amazonaws.com/v1/repos/ihf-rivendell-qa"
  },
  "author": "Importaciones Hiraoka",
  "license": "unlicensed",
  "devDependencies": {
    "@tsconfig/node18": "18.2.2",
    "@types/aws-lambda": "8.10.133",
    "@types/jest": "29.5.11",
    "@types/luxon": "3.4.2",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "husky-check-email": "0.0.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.525.0",
    "@aws-sdk/client-dynamodb": "^3.528.0",
    "@aws-sdk/client-lambda": "^3.525.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/client-secrets-manager": "^3.525.0",
    "@aws-sdk/lib-dynamodb": "^3.528.0",
    "@aws-sdk/util-buffer-from": "3.374.0",
    "@aws-sdk/util-utf8": "3.374.0",
    "@types/glob": "8.1.0",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "axios": "1.6.7",
    "bluebird": "^3.7.2",
    "csv-parse": "5.5.5",
    "csvtojson": "2.0.10",
    "glob": "10.3.10",
    "luxon": "3.4.4",
    "s3-sync-client": "4.3.1",
    "source-map-support": "0.5.21",
    "uuid": "9.0.1",
    "winston": "3.11.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-unknown",
      "eslint --fix --ext .ts"
    ]
  }
}
